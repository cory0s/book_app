<section>
    <% books.forEach(book => { %>
        <p>Title: <%= book.title %></p>
        <p>Author: <%= book.author %></p>
        <p>ISBN: <%= book.isbn %></p>
        <p>Image: <%= book.image_url %></p>
        <p>Description: <%= book.description %></p>
        <p>Bookshelf: <%= book.bookshelf %></p>
        <button> <a href="/books/<%=book.id%>"> View Detail </a> </button>
    <% }) %>
</section>